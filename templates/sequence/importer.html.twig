{% extends '@!EasyAdmin/page/content.html.twig' %}
{% block main %}
<div
	class="container-fluid">
	<!-- start page title -->
	<div class="row">
		<div class="col-12">
			<div class="page-title-box">
				<h4 class="page-title">{% trans %}Order{% endtrans %}</h4>
			</div>
		</div>
	</div>
	<!-- end page title -->

	<div class="row">
		<div class="col-lg-12">
			<div class="card">
				<div class="card-body">
					<div class="row">
						<div class="col-md-6">
					{{form_start(form)}}
					{{form_end(form)}}
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
{% endblock %}