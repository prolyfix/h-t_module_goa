{% extends '@!EasyAdmin/page/content.html.twig' %}
{% block main %}
	<div
		class="container-fluid">
		<!-- start page title -->
		<div class="row">
			<div class="col-12">
				<div class="page-title-box">
					<h4 class="page-title">{% trans %}Order{% endtrans %}</h4>
				</div>
			</div>
		</div>
		<!-- end page title -->

		<div class="row">
			<div class="col-lg-12">
				<div class="card">
					<div class="card-body">
                        {{form_start(form)}}
                        <div class="row">
                            <div class="col-md-6">  
                                {{form_row(form.defaultFactor)}}
                                {{form_row(form.entrySequence)}}
                            </div>
                            <div class="col-md-6">
                                {{form_row(form.compareSequence)}}
                                <input type="submit" class="btn btn-primary
                                waves-effect waves-light" value="validate"> 
                            </div>
                        </div>
						{{form_end(form)}}
                        <table class="table"> 
                            <tbody>

                            <tr>
                                <th>Go√Ñ</th>
                                <th>eingegebene Werte</th>
                                <th>Vergleich</th>
                            </tr>
                                {% for key in keys %}
                            <tr>
                                <td>{{key}}</td>
                                <td>
                                    {% if outputSequenceA[key] is defined %}
                                    <ul>
                                        {% for entrySequence in outputSequenceA[key] %}
                                            <li>Quantity:{{entrySequence.quantity}}, Faktor:{{entrySequence.factor}}</li>
                                        {% endfor %}
                                    </ul>
                                    {% endif %}
                                </td>
                                <td>
                                    {% if outputSequenceB[key] is defined %}
                                    <ul>
                                        {% for entrySequence in outputSequenceB[key] %}
                                            <li>Quantity:{{entrySequence.quantity}}, Faktor:{{entrySequence.factor}}</li>
                                        {% endfor %}
                                    </ul>
                                    {% endif %}                                
                                </td>
                            </tr>
                            {% endfor %}
                            </tbody>
                        </table>
					</div>
				</div>
			</div>
        </div>
    </div>
{% endblock %}
